{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "ecmaFeatures": {
    "modules":                      true,
    "experimentalObjectRestSpread": true,
    "destructuring": true,
    "spread": true,
  },
  "extends": "airbnb",
  "rules": {
    "react/no-unknown-property": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "spaced-comment": 0,
    "quotes": [2, "single"],
    "key-spacing":  0,
    "no-multi-spaces": 0,
    "no-console": 0,
    "consistent-return": 0,
    "prefer-arrow-callback": 0,
    "no-unused-expressions": 0,
    "dot-notation": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "no-eval": 1,
    "camelcase": 1,
    "react/prop-types": 0,
    "prop-types": 0
  }
}
